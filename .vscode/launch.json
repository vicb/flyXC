{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Windy Plugin",
      "request": "launch",
      "type": "chrome",
      "url": "https://windy.com/dev",
      "webRoot": "${workspaceFolder}/dist/libs/windy-souding"
    },
    {
      "name": "Attach to Chrome",
      "port": 9999,
      "request": "attach",
      "type": "chrome",
      "webRoot": "${workspaceFolder}/libs/windy-sounding"
    },
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}

{
  "name": "flyxc",
  "version": "1.1.0",
  "description": "flyxc.app",
  "main": "server.js",
  "scripts": {
    "start": "node app/server.js",
    "build": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c --environment NODE_ENV:'production'",
    "debug": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -wc",
    "lint": "eslint 'frontend/src/**/*.ts' --quiet --fix && eslint 'app/src/**/*.ts' --quiet --fix && eslint 'run/src/**/*.ts' --quiet --fix",
    "proto": "protoc --ts_out common/protos  --ts_opt long_type_number --ts_opt force_optimize_code_size --proto_path common/protos track.proto live-track.proto fetcher-state.proto",
    "emulator:start": "gcloud beta emulators datastore start --data-dir=ds",
    "emulator:env": "gcloud beta emulators datastore env-init --data-dir=ds",
    "emulator:unenv": "gcloud beta emulators datastore env-unset --data-dir=ds",
    "test": "jest",
    "chunks:clean": "node frontend/scripts/clean_chunks.js",
    "deploy": "npm run test && npm run build && npm run chunks:clean && gcloud app deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",

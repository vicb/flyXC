{
  "name": "flyxc",
  "version": "1.1.0",
  "description": "flyxc.app",
  "main": "server.js",
  "scripts": {
    "start": "node app/server.js",
    "build": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c --environment USE_CACHE,NODE_ENV:'production'",
    "debug": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -m inline -wc --environment USE_CACHE",
    "lint": "eslint 'frontend/src/**/*.ts' --quiet --fix && eslint 'app/src/**/*.ts' && eslint 'run/src/**/*.ts' --quiet --fix",
    "proto": "npx pbf track.proto > frontend/src/viewer/logic/track.js",
    "emulator:start": "gcloud beta emulators datastore start --data-dir=ds",
    "emulator:env": "gcloud beta emulators datastore env-init --data-dir=ds",
    "emulator:unenv": "gcloud beta emulators datastore env-unset --data-dir=ds"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.10.2",
    "@google-cloud/datastore": "^5.1.0",
    "@google-cloud/storage": "^5.0.1",
    "@koa/multer": "^3.0.0",
    "@tmcw/togeojson": "^4.0.0",
    "async-await-queue": "^1.1.1",
    "geojson": "^0.5.0",
    "google-auth-library": "^6.0.0",
    "google-polyline": "^1.0.3",
    "gpx-builder": "^3.2.0",
    "igc-parser": "0.5.0",
    "ioredis": "^4.17.3",
    "koa": "^2.12.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^4.0.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^9.0.1",
    "koa-static": "^5.0.0",
    "multer": "^1.4.2",
    "pbf": "^3.2.1",
    "printf": "^0.5.3",
    "qrcode": "^1.4.4",
    "request-zero": "^0.2.6",
    "simplify-path": "^1.1.0",
    "tiny-lru": "^7.0.4",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3",
    "xmlbuilder": "^15.1.1",
    "xmldom": "^0.3.0"
  },
  "devDependencies": {
    "@types/koa": "^2.11.3",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa-static": "^4.0.1",
    "@types/koa__multer": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-unstyled": "^1.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "rollup": "^2.11.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-minify-html-literals": "^1.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",

{
  "name": "proxy",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/proxy/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "dependsOn": [
        {
          "projects": ["secrets"],
          "target": "build"
        }
      ],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/proxy",
        "main": "apps/proxy/src/main.ts",
        "tsConfig": "apps/proxy/tsconfig.app.json",
        "generatePackageJson": true,
        "babelUpwardRootMode": true,
        "webpackConfig": "apps/proxy/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "proxy:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "proxy:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "dependsOn": [
        {
          "projects": ["secrets"],
          "target": "build"
        }
      ],
      "options": {
        "jestConfig": "apps/proxy/jest.config.ts"
      }
    },
    "container": {
      "executor": "@nx-tools/nx-container:build",
      "dependsOn": ["build"],
      "options": {
        "platforms": ["linux/amd64", "linux/arm64"],
        "engine": "docker",
        "push": true,
        "pull": true,
        "tags": ["us-docker.pkg.dev/fly-xc/docker/proxy"]
      }
    }
  }
}
